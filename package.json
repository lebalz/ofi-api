{
  "name": "ofi-backend",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "author": "Balthasar Hofer",
  "scripts": {
    "start": "npx ts-node -r dotenv/config server.ts",
    "build": "npx tsc -p .",
    "pretest": "NODE_ENV=test node ./node_modules/.bin/node-pg-migrate up --database-url-var=TEST_DATABASE_URL",
    "test": "jest",
    "dumpit": "npx ts-node -r dotenv/config dumpit.ts"
  },
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "ts-config": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/*.+(ts|tsx|js)"
    ]
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^1.9.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.175",
    "@types/morgan": "^1.9.3",
    "@types/passport-azure-ad": "^4.3.1",
    "bufferutil": "^4.0.4",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cors": "2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "morgan": "1.10.0",
    "node-pg-migrate": "^6.0.0",
    "nodemon": "^2.0.13",
    "passport": "^0.5.0",
    "passport-azure-ad": "^4.3.1",
    "pg": "^8.7.1",
    "socket.io": "^4.2.0",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3",
    "utf-8-validate": "^5.0.6"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/supertest": "^2.0.11",
    "jest": "^27.3.1",
    "passport-mock-strategy": "^2.0.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react": "^5.0.0"
  },
  "engines": {
    "node": "18.x"
  }
}
